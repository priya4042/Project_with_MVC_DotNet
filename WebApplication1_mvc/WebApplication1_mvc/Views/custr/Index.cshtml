@model IEnumerable<WebApplication1_mvc.Models.Customer>
@{
    ViewBag.Title = "Index";
}

<h2 class="text-info text-center">customer List</h2>

<table class="table table-striped table-bordered table-hover" >
    <thead>
        <tr>custno</tr>
        <tr>custname</tr>
        <tr>custaddress</tr>
        <tr>salary</tr>
    </thead>
    <tbody>
@foreach (var customer in Model)
            { <tr>
                <td>@customer.custno</td>
        <td>@customer.custname</td>
        <td>@customer.custadd</td>
        <td>@customer.salary</td>
              
        </tr>
            }


    </tbody>
</table>



