@model IEnumerable<web_mvc_12.Models.Employee>
@{
    ViewBag.Title = "Index";
}

<h2 class="text-center, text-center">Employee List</h2>
<br/>
<br/>
<table class="table table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>empno</th>
            <th>empname</th>
            <th>empaddress</th>
            <th>empsalary</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var emp in Model)
            {
                <tr>
                    <td>@emp.empno</td>
                    <td>@emp.empname</td>
                    <td>@emp.empaddress</td>
                    <td>@emp.empsalary</td>
                </tr>
            }
    </tbody>

</table>


